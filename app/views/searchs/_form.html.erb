<%= form_tag(search_path, method: :get) do %>
  <a><%= text_field_tag 'content' %></a>
  <a><%= select_tag 'model', options_for_select({ "ユーザー名" => "end_user", "投稿" => "post" }) %></a>
  <a><%= select_tag 'method', options_for_select({ "完全一致" => "perfect", "部分一致" => "partial" }) %></a>
  <%= submit_tag '検索' %>
<% end %>